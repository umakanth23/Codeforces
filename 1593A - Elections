#include<iostream>
using namespace std;
int main()
{

    int t;
    cin>>t;
    while(t--)
    {
        int a,b,c;
        cin>>a>>b>>c;
        int out[3] = {0};
        int x = max(b,c) - a +1;
        if(x<0)
        {
            x = 0;
        }
        int y = max(c,a) - b +1;
        if(y<0)
        {
            y = 0;
        }
        int z = max(a,b) - c +1;
        if(z<0)
        {
            z = 0;
        }
        cout<<x<<" "<<y<<" "<<z<<endl;
    }
}
